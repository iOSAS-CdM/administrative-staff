    "version:patch": "./scripts/bump-version.sh patch",
    "version:minor": "./scripts/bump-version.sh minor",
    "version:major": "./scripts/bump-version.sh major",
    "version:commit": "git add package.json package-lock.json && git commit -m \"chore: bump version to $(node -p 'require(\"./package.json\").version')\"",
    "release:patch": "npm run version:patch && npm run version:commit",
    "release:minor": "npm run version:minor && npm run version:commit",
    "release:major": "npm run version:major && npm run version:commit",
    "release": "git checkout release && git merge main && git push && git checkout main"
  },
