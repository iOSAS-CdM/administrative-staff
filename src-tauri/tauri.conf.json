{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "osas-staff",
  "version": "0.2.5",
  "identifier": "com.danieljohnbyns.osas-staff",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Office of Student Affairs and Services - Staff",
        "width": 1000,
        "height": 800,
        "minWidth": 800,
        "minHeight": 500,
        "zoomHotkeysEnabled": true,
        "backgroundColor": "#FFFFFF00"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": [
          "oauth",
          "osas-administrative-staff"
        ]
      }
    },
    "updater": {
      "endpoints": [
        "https://api.github.com/repos/iOSAS-CdM/administrative-staff/releases/latest"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDIxNEVGNzcwM0VENDZBOEUKUldTT2F0UStjUGRPSWJYYkhKYitnYkNtK2oyeGFRa1FFeHh4d"
    }
  }
}
