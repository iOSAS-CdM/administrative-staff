{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "osas-staff",
  "version": "../package.json",
  "identifier": "com.danieljohnbyns.osas-staff",
  "build": {
    "beforeDevCommand": "npm run dev",
	"devUrl": "http://localhost:1421",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
		"title": "Office of Student Affairs and Services - Staff",
		"width": 1200,
		"height": 800,
		"minWidth": 1200,
		"minHeight": 800,
		"zoomHotkeysEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
